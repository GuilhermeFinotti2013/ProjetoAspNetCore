@model ProjetoAspNetCore.Domain.Models.Paciente

@{
    ViewBag.Title = "Detalhes de Paciente: [ " + Model.Nome.ToUpper() + " ]";
    Layout = null;
}

<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <h4 class="modal-title">MedicalSystem</h4>
        </div>
        <div class="modal-body">
            <div class="form-horizontal">
                <div>
                    <h4> @ViewBag.Title </h4>
                    <hr />
                    <detalhar-dado Dado="@Model"></detalhar-dado>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <input type="button" value="Fechar" class="btn btn-default" data-dismiss="modal" />
        </div>
    </div>
</div>
