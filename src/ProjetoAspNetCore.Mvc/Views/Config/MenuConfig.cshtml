@{ ViewData["Title"] = "Configurações"; }

<vc:cabecalho titulo="SysMed - Controle hospitalar simples" subtitulo="@ViewData["Title"]"></vc:cabecalho>

<div class="row" style="margin-top: 12px;">
    <div class="col-lg-12">
        <p>
            @Html.ActionLink("Home", "Index", "Home", null, new { @class = "btn btn-default" })
        </p>

        <div class="animated fadeInRightBig">
            <div class="panel panel-primary toggle">
                <div class="panel-heading">
                    <h4 class="panel-title"><i class="fa fa-magic"></i>MedicalSystem - Configurações</h4>
                </div>
                <div class="panel-body">
                    <div class="jumbotron">
                        <a asp-controller="Config" asp-action="ImportMedicamentos" class="btn btn-info">Importar Medicamentos</a>
                        <a asp-controller="Config" asp-action="ImportarGenericos" class="btn btn-info">Importar Genérico</a>
                        <a asp-controller="Config" asp-action="ImportarCid" class="btn btn-info">Importar Cid</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
