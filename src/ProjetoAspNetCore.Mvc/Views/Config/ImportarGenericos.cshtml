@{ ViewData["Title"] = "Importar Genéricos"; }

<vc:cabecalho titulo="SysMed - Controle hospitalar simples" subtitulo="Importação de Genéricos"></vc:cabecalho>
<div class="row" style="margin-top: 12px;">
    <div class="col-lg-12">
        <p>
            @Html.ActionLink("Config", "Index", "Config", null, new { @class = "btn btn-default" })
        </p>

        <div class="animated fadeInRightBig">
            <div class="panel panel-primary toggle">
                <div class="panel-heading">
                    <h4 class="panel-title"><i class="fa fa-magic"></i>MedicalSystem - @ViewData["Title"]</h4>
                </div>
                <div class="panel-body">
                    <form asp-controller="Generico" asp-action="ImportGenerico" name="ImportGenerico" method="post"
                          enctype="multipart/form-data">
                        <input type="file" name="file" />
                        <hr />
                        <input type="submit" name="submit" id="submitGenerico" value="Carregar..." class="btn btn-info" />
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
