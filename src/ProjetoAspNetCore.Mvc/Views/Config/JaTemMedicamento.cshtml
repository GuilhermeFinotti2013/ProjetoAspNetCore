@model IEnumerable<ProjetoAspNetCore.Domain.Entities.Medicamento>



<div class="row" style="margin-top: 12px;">



    <div class="col-lg-12">

        <div class="animated fadeInRightBig">
            <div class="panel panel-primary toggle">
                <!-- Start .panel -->
                <div class="panel-heading">
                    <h4 class="panel-title"><i class="fa fa-magic"></i> MedicalManagement-Sys - Lista de Medicamentos</h4>
                </div>
                <div class="panel-body">

                    <div class="jumbotron">
                        <h3 class="alert alert-info">Os medicamantos Já foram importados!</h3>
                    </div>

                    <listar-dados Items="@Model"></listar-dados>


                </div>
            </div>
        </div>
    </div>
</div>

