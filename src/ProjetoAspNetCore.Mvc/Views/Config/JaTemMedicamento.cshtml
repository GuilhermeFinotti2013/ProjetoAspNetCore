<div class="modal fade modal-style6"
     id="modalGnerico"
     tabindex="-1"
     role="dialog"
     aria-labelledby="myModalLabel"
     aria-hidden="true">

</div>


@section Scripts{

    <script src="~/bundle/dataTables.min.js"></script>

    <script type="text/javascript">

        $(".details").click(function () {
            var id = $(this).attr("data-id");
            $("#modalGnerico").load("/Generico/Details?id=" + id, function () {
                $("#modalGnerico").modal();
            });
        });

        $(".delete").click(function () {
            var id = $(this).attr("data-id");
            $("#modalGnerico").load("/Generico/Delete?id=" + id, function () {
                $("#modalGnerico").modal();
            });
        });

        $(".edit").click(function () {
            var id = $(this).attr("data-id");
            $("#modalGnerico").load("/Generico/Edit?id=" + id, function () {
                $("#modalGnerico").modal();
            });
        });

        $(".create").click(function () {
            $("#modalGnerico").load("/Generico/Create", function () {
                $("#modalGnerico").modal();
            });
        });

    </script>
    <script>
        $(document).ready(function () {
            $('#myTable').DataTable({
                dom: 'T<"clear">lfrtip',
                tableTools: {
                    "sSwfPath": "/myPlugins/copy_csv_xls_pdf.swf"
                }
            });
        });
    </script>
}